    }
  ],
  "url": "https://github.com/garora/number2text/blob/master/LICENSE",
  "bugs": {
    "url": "https://github.com/garora/number2text/issues"
  },
  "homepage": "https://github.com/garora/number2text",
  "devDependencies": {
    "mocha": "^2.2.1",
    "chai": "^2.2.0",
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13"
  }
